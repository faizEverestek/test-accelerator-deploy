{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run FastAPI with Uvicorn",
        "type": "debugpy",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--host", "0.0.0.0",
          "--port", "8085"
        ],
        "env": {
          "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/src:${workspaceFolder}/test"
        },
        "console": "integratedTerminal"
      }
    ]
}
  